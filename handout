<script type="text/javascript" language="javascript">
if (document.getElementById('j_username').value.length == 0) { 
  document.getElementById('j_username').focus();
} else {
  document.getElementById('j_password').focus();
}
document.getElementById('bar').style.height=document.getElementById('loginformdiv').clientHeight*2/3 + "px";
document.getElementById('bar').style.marginTop=document.getElementById('loginformdiv').clientHeight*1/6 + "px";
document.getElementById('bar').style.marginBottom=document.getElementById('loginformdiv').clientHeight*1/6 + "px";

document.getElementById('library').style.height=document.getElementById('loginformdiv').clientHeight + "px";

function resizelibrary() {
var maxw = document.getElementById('maincontent').clientWidth - document.getElementById('loginformdiv').clientWidth - document.getElementById('bar').clientWidth - 200;
var formw = document.getElementById('loginformdiv').clientWidth;
var minw = 300;

var libw = minw;
document.getElementById('library').style.width = libw + "px";

}
window.onresize=resizelibrary();
resizelibrary();

</script>
